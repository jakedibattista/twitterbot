# X API Credentials
# Get these from https://developer.twitter.com/en/portal/dashboard
X_API_KEY=your_twitter_api_key_here
X_API_SECRET=your_twitter_api_secret_here
X_ACCESS_TOKEN=your_twitter_access_token_here
X_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here

# Google Sheets Configuration
GOOGLE_SHEETS_CREDENTIALS_PATH=config/service_account.json
GOOGLE_SHEETS_ID=your_google_spreadsheet_id_here

# Optional: AI Services for Enhanced Features
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Optional: Google Custom Search (for more reliable LinkedIn discovery)
GOOGLE_CSE_API_KEY=your_google_custom_search_api_key_here
GOOGLE_CSE_CX=your_custom_search_engine_id_here

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=development